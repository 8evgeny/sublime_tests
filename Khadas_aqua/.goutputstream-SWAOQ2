#include <iostream>
#include <stdio.h>
#include <wiringPi.h>
#include <wiringPiI2C.h>

const int gpio_pin_LAMP = 420;

int main () 
{
	if(-1 == wiringPiSetup())
	{
		printf("set up error");
		exit(1);
	}
	pinMode(gpio_pin_LAMP, OUTPUT);
	
	
    std::cout <<"aqua programm..."<<std::endl; 
    system("gpio readall");
    
	while(1)
	{
		digitalWrite(gpio_pin_LAMP, HIGH);
		printf("gpio_pin_LAMP ON\n");
		delay(1000);
		digitalWrite(gpio_pin_LAMP, LOW);
		printf("gpio_pin_LAMP OFF\n");
		delay(1000);
	}
    
    
    return 0;

} 
